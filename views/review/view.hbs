<h1>Review</h1>
<img src="{{image}}" alt="image from blog {{blogName}}" width="150" />
<section>
  <img src="{{albumBiggestImage}}" alt="Album cover of {{name}}" width="300" />
  <h4>{{rating}}/10</h4>
  <h2>{{name}}</h2>
  <h3>by {{blogName}}</h3>
  <a href="/review/{{albumId}}/{{reviewId}}/edit">EDIT</a>
  <form action="/review/{{albumId}}/{{reviewId}}/delete">
    <button>DELETE REVIEW</button>
  </form>
</section>

<section>
  <h2>{{subheading}}</h2>
  <p>{{content}}</p>
</section>